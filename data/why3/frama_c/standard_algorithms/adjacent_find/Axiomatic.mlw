predicate p_hasequalneighbors (mint_0 : map addr int) (a : addr) (n : int) =
    exists i : int.
    (mint_0[(shift_sint32 a (1 + i))] = mint_0[(shift_sint32 a i)]) /\
    (0 <= i) /\ ((2 + i) <= n)

end

