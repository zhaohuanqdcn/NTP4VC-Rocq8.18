theorem search'vc (m : Matrix.matrix ℤ) (v : ℤ) (fact0 : ∀(i : ℤ), (0 : ℤ) ≤ i ∧ i < Matrix.rows m → (∀(j1 : ℤ) (j2 : ℤ), (0 : ℤ) ≤ j1 ∧ j1 ≤ j2 ∧ j2 < Matrix.columns m → Matrix.get m i j1 ≤ Matrix.get m i j2)) (fact1 : ∀(j : ℤ), (0 : ℤ) ≤ j ∧ j < Matrix.columns m → (∀(i1 : ℤ) (i2 : ℤ), (0 : ℤ) ≤ i1 ∧ i1 ≤ i2 ∧ i2 < Matrix.rows m → Matrix.get m i1 j ≤ Matrix.get m i2 j)) : let o1 : ℤ := Matrix.columns m - (1 : ℤ); (((0 : ℤ) ≤ (0 : ℤ) ∧ (0 : ℤ) ≤ Matrix.rows m) ∧ (-(1 : ℤ) ≤ o1 ∧ o1 < Matrix.columns m) ∧ (∀(i' : ℤ) (j' : ℤ), (0 : ℤ) ≤ i' ∧ i' < Matrix.rows m → (0 : ℤ) ≤ j' ∧ j' < Matrix.columns m → i' < (0 : ℤ) ∨ o1 < j' → ¬Matrix.get m i' j' = v)) ∧ (∀(j : ℤ) (i : ℤ), ((0 : ℤ) ≤ i ∧ i ≤ Matrix.rows m) ∧ (-(1 : ℤ) ≤ j ∧ j < Matrix.columns m) ∧ (∀(i' : ℤ) (j' : ℤ), (0 : ℤ) ≤ i' ∧ i' < Matrix.rows m → (0 : ℤ) ≤ j' ∧ j' < Matrix.columns m → i' < i ∨ j < j' → ¬Matrix.get m i' j' = v) → (∀(o2 : Bool), (if i < Matrix.rows m then o2 = (if (0 : ℤ) ≤ j then true else false) else o2 = false) → (if o2 = true then Matrix.valid_index m i j ∧ (let x : ℤ := Matrix.elts m i j; if x = v then ∃(i1 : ℤ) (j1 : ℤ), ((0 : ℤ) ≤ i1 ∧ i1 < Matrix.rows m) ∧ ((0 : ℤ) ≤ j1 ∧ j1 < Matrix.columns m) ∧ Matrix.get m i1 j1 = v else if x < v then ((0 : ℤ) ≤ Matrix.rows m - i + j ∧ Matrix.rows m - (i + (1 : ℤ)) + j < Matrix.rows m - i + j) ∧ ((0 : ℤ) ≤ i + (1 : ℤ) ∧ i + (1 : ℤ) ≤ Matrix.rows m) ∧ (-(1 : ℤ) ≤ j ∧ j < Matrix.columns m) ∧ (∀(i' : ℤ) (j' : ℤ), (0 : ℤ) ≤ i' ∧ i' < Matrix.rows m → (0 : ℤ) ≤ j' ∧ j' < Matrix.columns m → i' < i + (1 : ℤ) ∨ j < j' → ¬Matrix.get m i' j' = v) else ((0 : ℤ) ≤ Matrix.rows m - i + j ∧ Matrix.rows m - i + (j - (1 : ℤ)) < Matrix.rows m - i + j) ∧ ((0 : ℤ) ≤ i ∧ i ≤ Matrix.rows m) ∧ (-(1 : ℤ) ≤ j - (1 : ℤ) ∧ j - (1 : ℤ) < Matrix.columns m) ∧ (∀(i' : ℤ) (j' : ℤ), (0 : ℤ) ≤ i' ∧ i' < Matrix.rows m → (0 : ℤ) ≤ j' ∧ j' < Matrix.columns m → i' < i ∨ j - (1 : ℤ) < j' → ¬Matrix.get m i' j' = v)) else ¬(∃(i1 : ℤ) (j1 : ℤ), ((0 : ℤ) ≤ i1 ∧ i1 < Matrix.rows m) ∧ ((0 : ℤ) ≤ j1 ∧ j1 < Matrix.columns m) ∧ Matrix.get m i1 j1 = v))))
