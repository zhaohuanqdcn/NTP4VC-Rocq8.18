predicate p_heapmaximumchild (mint_0 : map addr int) (a : addr) (n : int)
    (p : int) (c : int) =
    let x = (l_heapleft p) in let x_1 = mint_0[(shift_sint32 a c)] in
    let x_2 = (l_heapright p) in (0 <= p) /\ ((2 + p) <= n) /\
    ((p <= 2147483646) -> (((l_heapparent c)) = p)) /\
    (((2 + x) <= n) -> (mint_0[(shift_sint32 a x)] <= x_1)) /\
    (((2 + x_2) <= n) -> (mint_0[(shift_sint32 a x_2)] <= x_1))

