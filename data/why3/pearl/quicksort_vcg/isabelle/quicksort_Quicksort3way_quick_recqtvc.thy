definition qs_partition :: "int list \<Rightarrow> int list \<Rightarrow> int \<Rightarrow> int \<Rightarrow> int \<Rightarrow> int \<Rightarrow> int \<Rightarrow> _"
  where "qs_partition a1 a2 l ml mr r v \<longleftrightarrow> permut_sub' a1 a2 (nat l) (nat r) \<and> (\<forall>(j :: int). l \<le> j \<and> j < ml \<longrightarrow> a2 ! nat j < v) \<and> (\<forall>(j :: int). ml \<le> j \<and> j < mr \<longrightarrow> a2 ! nat j = v) \<and> (\<forall>(j :: int). mr \<le> j \<and> j < r \<longrightarrow> v < a2 ! nat j)" for a1 a2 l ml mr r v
theorem quick_rec'vc:
  fixes l :: "int"
  fixes r :: "int"
  fixes a :: "int list"
  assumes fact0: "(0 :: int) \<le> l"
  assumes fact1: "l \<le> r"
  assumes fact2: "r \<le> int (length a)"
  shows "if l + (1 :: int) < r then ((0 :: int) \<le> l \<and> l < int (length a)) \<and> (let v :: int = a ! nat l; o1 :: int = l + (1 :: int) in ((l \<le> l \<and> l < o1 \<and> o1 \<le> r \<and> r \<le> r) \<and> (\<forall>(j :: int). l \<le> j \<and> j < l \<longrightarrow> a ! nat j < v) \<and> (\<forall>(j :: int). l \<le> j \<and> j < o1 \<longrightarrow> a ! nat j = v) \<and> (\<forall>(j :: int). r \<le> j \<and> j < r \<longrightarrow> v < a ! nat j) \<and> permut_sub' a a (nat l) (nat r)) \<and> (\<forall>(i :: int) (mr :: int) (ml :: int) (a1 :: int list). length a1 = length a \<longrightarrow> (l \<le> ml \<and> ml < i \<and> i \<le> mr \<and> mr \<le> r) \<and> (\<forall>(j :: int). l \<le> j \<and> j < ml \<longrightarrow> a1 ! nat j < v) \<and> (\<forall>(j :: int). ml \<le> j \<and> j < i \<longrightarrow> a1 ! nat j = v) \<and> (\<forall>(j :: int). mr \<le> j \<and> j < r \<longrightarrow> v < a1 ! nat j) \<and> permut_sub' a a1 (nat l) (nat r) \<longrightarrow> (if i < mr then ((0 :: int) \<le> i \<and> i < int (length a1)) \<and> (if a1 ! nat i < v then (((0 :: int) \<le> ml \<and> ml < int (length a1)) \<and> (0 :: int) \<le> i \<and> i < int (length a1)) \<and> (\<forall>(a2 :: int list). length a2 = length a1 \<longrightarrow> list_exchange a1 a2 ml i \<longrightarrow> ((0 :: int) \<le> mr - i \<and> mr - (i + (1 :: int)) < mr - i) \<and> (l \<le> ml + (1 :: int) \<and> ml + (1 :: int) < i + (1 :: int) \<and> i + (1 :: int) \<le> mr \<and> mr \<le> r) \<and> (\<forall>(j :: int). l \<le> j \<and> j < ml + (1 :: int) \<longrightarrow> a2 ! nat j < v) \<and> (\<forall>(j :: int). ml + (1 :: int) \<le> j \<and> j < i + (1 :: int) \<longrightarrow> a2 ! nat j = v) \<and> (\<forall>(j :: int). mr \<le> j \<and> j < r \<longrightarrow> v < a2 ! nat j) \<and> permut_sub' a a2 (nat l) (nat r)) else ((0 :: int) \<le> i \<and> i < int (length a1)) \<and> (if v < a1 ! nat i then (((0 :: int) \<le> i \<and> i < int (length a1)) \<and> (0 :: int) \<le> mr - (1 :: int) \<and> mr - (1 :: int) < int (length a1)) \<and> (\<forall>(a2 :: int list). length a2 = length a1 \<longrightarrow> list_exchange a1 a2 i (mr - (1 :: int)) \<longrightarrow> ((0 :: int) \<le> mr - i \<and> mr - (1 :: int) - i < mr - i) \<and> (l \<le> ml \<and> ml < i \<and> i \<le> mr - (1 :: int) \<and> mr - (1 :: int) \<le> r) \<and> (\<forall>(j :: int). l \<le> j \<and> j < ml \<longrightarrow> a2 ! nat j < v) \<and> (\<forall>(j :: int). ml \<le> j \<and> j < i \<longrightarrow> a2 ! nat j = v) \<and> (\<forall>(j :: int). mr - (1 :: int) \<le> j \<and> j < r \<longrightarrow> v < a2 ! nat j) \<and> permut_sub' a a2 (nat l) (nat r)) else ((0 :: int) \<le> mr - i \<and> mr - (i + (1 :: int)) < mr - i) \<and> (l \<le> ml \<and> ml < i + (1 :: int) \<and> i + (1 :: int) \<le> mr \<and> mr \<le> r) \<and> (\<forall>(j :: int). l \<le> j \<and> j < ml \<longrightarrow> a1 ! nat j < v) \<and> (\<forall>(j :: int). ml \<le> j \<and> j < i + (1 :: int) \<longrightarrow> a1 ! nat j = v) \<and> (\<forall>(j :: int). mr \<le> j \<and> j < r \<longrightarrow> v < a1 ! nat j) \<and> permut_sub' a a1 (nat l) (nat r))) else (((0 :: int) \<le> r - l \<and> ml - l < r - l) \<and> (0 :: int) \<le> l \<and> l \<le> ml \<and> ml \<le> int (length a1)) \<and> (\<forall>(a2 :: int list). length a2 = length a1 \<longrightarrow> sorted_sub1 a2 l ml \<and> permut_sub' a1 a2 (nat l) (nat ml) \<longrightarrow> (((0 :: int) \<le> r - l \<and> r - mr < r - l) \<and> (0 :: int) \<le> mr \<and> mr \<le> r \<and> r \<le> int (length a2)) \<and> (\<forall>(a3 :: int list). length a3 = length a2 \<longrightarrow> sorted_sub1 a3 mr r \<and> permut_sub' a2 a3 (nat mr) (nat r) \<longrightarrow> sorted_sub1 a3 l r \<and> permut_sub' a a3 (nat l) (nat r)))))) else sorted_sub1 a l r \<and> permut_sub' a a (nat l) (nat r)"
  sorry
