nd> (\<forall>(k :: int). (0 :: int) \<le> k \<and> k < i + (1 :: int) \<longrightarrow> a2 ! nat k = False) \<and> (\<forall>(k :: int). j - (1 :: int) < k \<and> k < int (length a2) \<longrightarrow> a2 ! nat k = True) \<and> a <~~> a2))) else sorted a1 \<and> a <~~> a1))"
