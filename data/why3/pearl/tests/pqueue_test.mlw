module Test

  use seq.Seq
  use import pqueue.Pqueue as P

  val constant v1 : elt
  val constant v2 : elt
  axiom values: le v2 v1 /\ not (le v1 v2)

  let test0 () =
    let s = P.create () : P.t in
    let b = P.is_empty s in
    let n = P.length s in
    ()

  let test1 () =
    raises { P.Empty }
    let s = P.create () in
    P.push v1 s;
    let x = P.peek s in
    P.push v2 s;
    let x = P.peek s in
    ()

end

