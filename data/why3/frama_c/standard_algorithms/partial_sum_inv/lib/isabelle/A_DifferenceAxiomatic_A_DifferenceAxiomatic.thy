theory A_DifferenceAxiomatic_A_DifferenceAxiomatic
  imports "NTP4Verif.NTP4Verif" "Why3STD.Qed_Qed" "Why3STD.Memory_Memory" "Why3STD.Cint_Cint" "../../lib/isabelle/Compound_Compound" "../../lib/isabelle/Axiomatic_Axiomatic"
begin
consts l_difference :: "(addr \<Rightarrow> int) \<Rightarrow> addr \<Rightarrow> int \<Rightarrow> int"
axiomatization where Q_TL_Difference:   "is_sint32 (l_difference mint_0 a n)"
  for mint_0 :: "addr \<Rightarrow> int"
  and a :: "addr"
  and n :: "int"
axiomatization where Q_DifferenceEmptyOrSingle:   "mint_0 (shift a (0 :: int)) = l_difference mint_0 a n"
 if "n \<le> (0 :: int)"
  for n :: "int"
  and mint_0 :: "addr \<Rightarrow> int"
  and a :: "addr"
axiomatization where Q_DifferenceNext:   "mint_0 (shift a n) = l_difference mint_0 a n + mint_0 (shift a (n - (1 :: int)))"
 if "(0 :: int) < n"
  for n :: "int"
  and mint_0 :: "addr \<Rightarrow> int"
  and a :: "addr"
axiomatization where Q_DifferenceRead:   "l_difference mint_1 a n = l_difference mint_0 a n"
 if "p_unchanged_1' mint_0 mint_1 a (0 :: int) ((1 :: int) + n)"
  for mint_0 :: "addr \<Rightarrow> int"
  and mint_1 :: "addr \<Rightarrow> int"
  and a :: "addr"
  and n :: "int"
end
