function l_scalar_product (v1_0 : s9_realvect3_s)
    (v2_0 : s9_realvect3_s) : real =
    ((v1_0.f9_realvect3_s_x) *. (v2_0.f9_realvect3_s_x))
    +. ((v1_0.f9_realvect3_s_y) *. (v2_0.f9_realvect3_s_y))
    +. ((v1_0.f9_realvect3_s_z) *. (v2_0.f9_realvect3_s_z))

